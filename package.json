{
  "name": "imgcap",
  "version": "1.0.2",
  "description": "Automatically compress images to exact file size using binary search algorithm.",
  "type": "module",
  "main": "core/dist/index.js",
  "scripts": {
    "dev": "pnpm --filter core dev",
    "build": "pnpm --filter core build",
    "serve": "serve",
    "lint": "npm-run-all -p lint:*",
    "check": "npm-run-all -p check:*",
    "test": "pnpm --filter __tests__ test",
    "lint:core": "pnpm --filter core lint",
    "lint:tests": "pnpm --filter __tests__ lint",
    "check:core": "pnpm --filter core check",
    "check:tests": "pnpm --filter __tests__ check",
    "prepare": "husky"
  },
  "keywords": [
    "image",
    "compression",
    "compress",
    "resize",
    "optimize",
    "binary-search",
    "file-size",
    "target-size",
    "precise-size",
    "webp",
    "avif",
    "jpeg",
    "png",
    "upload",
    "quality",
    "canvas",
    "offscreen-canvas"
  ],
  "author": "molvqingtai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/molvqingtai/imgcap.git"
  },
  "bugs": {
    "url": "https://github.com/molvqingtai/imgcap/issues"
  },
  "homepage": "https://github.com/molvqingtai/imgcap#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^24.2.7",
    "serve": "^14.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
